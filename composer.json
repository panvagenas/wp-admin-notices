{
    "name": "panvagenas/wp-admin-notices",
    "description": "A simple script to display WordPress notices to admin page",
    "license": "GPL-3.0+",
    "version": "2.0.0",
    "authors": [
        {
            "name": "Panagiotis Vagenas",
            "email": "pan.vagenas@gmail.com",
            "homepage": "https://gr.linkedin.com/in/panvagenas",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "codacy/coverage": "dev-master"
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "files": ["src/init.php"],
        "psr-4": {
            "Notices\\": "src"
        }
    },
    "archive": {
        "exclude": [
            "/docs",
            "/tests"
        ]
    }

}